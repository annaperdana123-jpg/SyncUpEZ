{
  "name": "syncup-ez",
  "version": "1.0.0",
  "description": "CSV-based implementation of SyncUp Continuous Contribution Graph",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "supabase:init": "node scripts/initSupabaseSchema.js",
    "supabase:migrate": "node scripts/migrateDataToSupabase.js"
  },
  "keywords": [
    "syncup",
    "csv",
    "employee-engagement",
    "analytics"
  ],
  "author": "SyncUp Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.56.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "proper-lockfile": "^4.1.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "axios": "^1.11.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.4"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
